    <style>
      label{
        display: block;
        margin-bottom: .25rem;
      }
      input {
        margin-bottom: 1em;
        border: 1px solid #ebebeb;
        border-radius: 5px;
        padding:.5em 1em;
      }
      .mce_inline_error{
        background: #FEE5E9 !important;
        color: #333 !important;
      }
      #mc_embed_signup form{
        padding: 0 !important;
      }

      
      h3 {
        font-size:1.5rem;
        margin: 1em 0 0 0;
        font-weight: lighter;
      }
      ul {
        line-height: 1.5 !important;
      }
      ul li {
        margin: 1em 0;
      }
      html,body,p{
        line-height: 1.5;
      }
      .home .navpad{
        padding-top: 0;
      }

      img {
        max-width: 100%;
      }
      p{
        margin: 1.5em 0 1em 0;
      }
      section ul, section ol{
        padding-left:1em;

      }
      .container{
        max-width: 960px;
      }
      
      section ul, section ul li { 
   list-style-type: disc; 
   list-style-position: inside; 
}
section ol, section ol li { 
   list-style-type: decimal; 
   list-style-position: inside; 
}
section ul ul, section ol ul { 
   list-style-type: circle; 
   list-style-position: inside; 
   margin-left: 15px; 
}
section ol ol, section ul ol { 
   list-style-type: lower-latin; 
   list-style-position: inside; 
   margin-left: 15px; 
}
    </style>
  </head>
  <body>

     <!-- Header Area wrapper Starts -->
    <header id="home" class="header">
